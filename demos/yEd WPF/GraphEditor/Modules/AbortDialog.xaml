<Window x:Class="Demo.yFiles.GraphEditor.Modules.Layout.AbortDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Layout" Width="320" Height="140" WindowStartupLocation="CenterOwner">
    <StackPanel Margin="0,10">
        <Label Name="label" Content="Layout in Progress" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <ProgressBar IsIndeterminate="True" Width="120" Height="10" x:Name="graphLoadingBar" Margin="0,10"/>
        <Button Name="stopButton" HorizontalAlignment="Center" Width="120" Margin="0" VerticalAlignment="Center" Click="OnStopClicked">Stop</Button>
    </StackPanel>
</Window>
